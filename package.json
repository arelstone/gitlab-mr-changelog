{
  "name": "os-changelog",
  "description": "Changelog generator for Ordrestyring",
  "version": "0.0.0",
  "author": "Carsten Daureh√∏j <arelstone@gmail.com>",
  "bin": {
    "os-changelog": "./bin/run"
  },
  "bugs": "https://github.com/Projects/os-changelog/issues",
  "dependencies": {
    "@gitbeaker/node": "^25.2.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^2.2.3",
    "inquirer": "^7.3.3",
    "json2md": "^1.9.0",
    "moment": "^2.29.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "@types/chai": "^4.2.14",
    "@types/inquirer": "^7.3.1",
    "@types/json2md": "^1.5.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.44",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Projects/os-changelog",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "os-changelog",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Projects/os-changelog",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'"
  },
  "types": "lib/index.d.ts"
}
